pipeline {
    agent any

    stages {
        stage('SCM Checkout') {
            steps {
                git url:'https://github.com/Abhishek24094/dev.git'
            }
        }
        stage('SonarQube Analysis')
        {
         withSonarQubeEnv('sonar')
         {
             sh "python hello_world.py sonar:sonar"
         }
        }
    }
}
